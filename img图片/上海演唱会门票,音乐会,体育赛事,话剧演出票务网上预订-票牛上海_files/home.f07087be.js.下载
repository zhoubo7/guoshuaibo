  function CountDown(timeLeft) {
    var _this = this;

    _classCallCheck(this, CountDown);

    this.timeLeft = timeLeft * 1000;
    this.endTime = new Date().getTime() + this.timeLeft;
    this.interval = setInterval(function () {
      _this.calTime();
    }, 1000);

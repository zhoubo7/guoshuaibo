  function colorForLi(li) {
    // 周年庆banner判断
    var color = li.attr('data-color');

    var _colorConvert$hex$rgb = _colorConvert2['default'].hex.rgb(color.slice(1, 7)),
        r = _colorConvert$hex$rgb[0],
        g = _colorConvert$hex$rgb[1],
